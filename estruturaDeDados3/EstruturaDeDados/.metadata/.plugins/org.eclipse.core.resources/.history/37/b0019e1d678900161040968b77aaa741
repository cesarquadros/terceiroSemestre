
public class Fibonacci {

    static int fibo(int numero) {
        if (numero < 2) {
            return numero;
        } else {
            return fibo(numero - 1) + fibo(numero - 2);
        }
    }
	
	public static void main(String[] args){
		
		System.out.println(calculoFibonnaci(6));
	}
	
	
	

}