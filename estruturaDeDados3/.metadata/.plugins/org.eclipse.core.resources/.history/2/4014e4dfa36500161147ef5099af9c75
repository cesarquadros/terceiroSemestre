
public class exercicio1 {
	
	public static void main(String[] args) {
		
	}
	
	static void imprimeQuatro(){
		int [] vetor = {1,2,3,4};
		for (int i = 0; i < vetor.length; i++) {
			System.out.println(vetor[i]);
		}
	}
	
	static void imprimeVetorN(int vetor[]){
		for (int i = 0; i < vetor.length; i++) {
			System.out.println(vetor[i]);
		}
	}

}
